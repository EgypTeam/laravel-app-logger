{
  "name": "egypteam/laravel-app-logger",
  "description": "Provider-agnostic structured application logger for Laravel (CloudWatch, etc.), with Log4j-like facade.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Pedro Ferreira",
      "email": "you@example.com"
    }
  ],
  "require": {
    "php": "^8.2",
    "illuminate/support": "^10.0|^11.0|^12.0",
    "monolog/monolog": "^3.0",
    "aws/aws-sdk-php": "^3.300"
  },
  "autoload": {
    "psr-4": {
      "Egypteam\\LaravelAppLogger\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Egypteam\\LaravelAppLogger\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Egypteam\\LaravelAppLogger\\LaravelAppLoggerServiceProvider"
      ],
      "aliases": {
        "AppLog": "Egypteam\\LaravelAppLogger\\Facades\\AppLog"
      }
    }
  },
  "minimum-stability": "stable",
  "require-dev": {
    "orchestra/testbench": "^9.0",
    "phpunit/phpunit": "^11.0",
    "mockery/mockery": "^1.6",
    "squizlabs/php_codesniffer": "^3.9"
  },
  "scripts": {
    "test": "phpunit",
    "cs": "phpcs --standard=PSR12 src"
  }
}
